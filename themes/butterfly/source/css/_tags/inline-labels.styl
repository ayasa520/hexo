u
  color: #444
  text-decoration: none
  border-bottom: 1px solid #FE5F58
emp
  color: #444
  border-bottom: 4px dotted #FE5F58
wavy
  color: #444
  text-decoration-style: wavy
  text-decoration-line: underline
  text-decoration-color: #FE5F58
psw
  color: transparent
  background: mix(#444, #fff, 50)
  border-radius: 2px
  trans()
